<div class="container">
  <div class="row">
    <div class="col-lg-9 col-md-9 col-sm-9">
      <div class="panel panel-default">
        <div class="panel-heading">
          Here is your insurance
        </div>

        <div class="panel-body">
          <%= form_for(@insurance) do |f| %>
            <div>First Name: <%#= @insurance.first_name %></div>
            <div>Last Name: <%#= @insurance.last_name %></div>
            <div>Enrollee ID: <%= @insurance.enrollee_id %></div>
            <div>Group Number: <%= @insurance.group_number %></div>

            <hr/>
            <div>
              <p>
                <%= link_to 'Edit my Insurance', edit_insurance_path(@insurance), class: "btn btn-primary" %>
              </p>
            </div>
          <% end %>
        </div>

      </div>

    </div>
  </div>
</div>

