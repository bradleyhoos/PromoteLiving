<h1 class="center">Here is your insurance</h1>
<h2> </h2>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@insurance) do |f| %>
      <%= f.label :first_name %>
      <%= f.text_field :first_name %>

      <%= f.label :last_name %>
      <%= f.text_field :last_name %>

      <%= f.label :enrollee_id %>
      <%= f.text_field :enrollee_id %>

      <%= f.label :group_number %>
      <%= f.text_field :group_number %>

      <%= f.submit "Update my Insurance", class: "btn btn-primary" %>
      <%= link_to "Delete Insurance", @insurance, method: :delete, data: {confirm: "Are you sure you'd like to delete this Insurance?"} %> |
    <% end %>
  </div>

</div>
