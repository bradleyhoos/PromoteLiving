<h1>Add your insurance</h1>
<h2>it should all come from your card </h2>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@insurance) do |f| %>
      <%= f.label :enrollee_id %>
      <%= f.text_field :enrollee_id %>


      <%= f.label :group_number %>
      <%= f.text_field :group_number %>

      <%= f.label :user_id %>
      <%= f.text_field :user_id, value: current_user.id %>

      <%= f.submit "Add my Insurance", class: "btn btn-primary" %>

    <% end %>
  </div>

</div>
