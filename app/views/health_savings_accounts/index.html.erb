<p id="notice"><%= notice %></p>

<h1>Listing Health Savings Accounts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @health_savings_accounts.each do |health_savings_account| %>
      <tr>
        <td><%= link_to 'Show', health_savings_account %></td>
        <td><%= link_to 'Edit', edit_health_savings_account_path(health_savings_account) %></td>
        <td><%= link_to 'Destroy', health_savings_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Health savings account', new_health_savings_account_path %>
